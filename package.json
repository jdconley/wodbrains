{
  "name": "wodbrains",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "pnpm run _test",
    "_test": "pnpm -r test",
    "test:e2e": "pnpm run _test:e2e",
    "_test:e2e": "E2E_WEB_PORT=5174 E2E_WORKER_PORT=8788 pnpm -w exec playwright test --config /Users/jd/src/wodbrains/playwright.config.ts",
    "test:readme": "pnpm run _test:readme",
    "_test:readme": "E2E_WEB_PORT=5174 E2E_WORKER_PORT=8788 pnpm -w exec playwright test --config /Users/jd/src/wodbrains/playwright.readme.config.ts",
    "test:live-evals": "pnpm run _test:live-evals",
    "_test:live-evals": "set -a && . \"apps/worker/.dev.vars\" && set +a && RUN_LIVE_AI_TESTS=1 pnpm --filter worker test -- test/parse.evals.test.ts test/parse.gemini.test.ts",
    "wrangler:gen": "dotenvx run --quiet -f .env.local -f .env -- pnpm run _wrangler:gen",
    "_wrangler:gen": "node scripts/generate-wrangler-config.mjs",
    "dev": "pnpm run _dev",
    "_dev": "concurrently -n web,worker -c magenta,cyan \"pnpm --filter web dev\" \"pnpm --filter worker dev\"",
    "dev:web": "pnpm run _dev:web",
    "_dev:web": "pnpm --filter web dev",
    "dev:worker": "pnpm run _dev:worker",
    "_dev:worker": "pnpm --filter worker dev",
    "build": "pnpm run _build",
    "_build": "pnpm -r build",
    "typecheck": "pnpm run _typecheck",
    "_typecheck": "pnpm -r --if-present run typecheck",
    "lint": "pnpm -w exec eslint .",
    "lint:fix": "pnpm -w exec eslint . --fix",
    "lint:strict": "pnpm -w exec eslint . --max-warnings=0",
    "format": "pnpm run _format",
    "_format": "prettier -w .",
    "format:check": "pnpm run _format:check",
    "_format:check": "prettier -c .",
    "prepare": "husky",
    "check": "pnpm run _check",
    "_check": "pnpm typecheck && pnpm lint && pnpm format:check",
    "db:migrate:local": "pnpm run _db:migrate:local",
    "_db:migrate:local": "pnpm --filter worker db:migrate:local",
    "db:generate:auth": "pnpm run _db:generate:auth",
    "_db:generate:auth": "pnpm --filter worker db:generate:auth",
    "deploy": "pnpm run _deploy",
    "_deploy": "pnpm --filter web run build && pnpm --filter worker run deploy",
    "deploy:prod": "pnpm run _deploy:prod",
    "_deploy:prod": "pnpm run deploy"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.28.1+sha512.7d7dbbca9e99447b7c3bf7a73286afaaf6be99251eb9498baefa7d406892f67b879adb3a1d7e687fc4ccc1a388c7175fbaae567a26ab44d1067b54fcb0d6a316",
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.12.6",
    "@dotenvx/dotenvx": "^1.52.0",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.58.0",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitest/runner": "^3.2.4",
    "@vitest/snapshot": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.1.0",
    "husky": "^9.1.7",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vitest": "^3.2.4"
  }
}
